service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read;
    }
    match /sections/{sectionID} {
      allow write: if request.auth.uid != null;
    }
        match /arguments/{argumentId} {
      allow write: if request.auth.uid != null;
    }
        match /consenz/{consenzId} {
      allow write: if request.auth.uid != null;
    }
        match /documents/{documentID} {
      allow write: if request.auth.uid != null;
    }
        match /users/{userID} {
      allow write;
    }
  }
}
